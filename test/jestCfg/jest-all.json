{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "../../",
    "testMatch": ["**/*.spec.ts", "**/*.e2e-spec.ts"],
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": ["src/**/*.(t|j)s"],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
}